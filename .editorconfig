# Using https://google.github.io/styleguide/javaguide.html, download from https://github.com/google/styleguide

root = true

[*]
charset = utf-8
end_of_line = LF
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false
